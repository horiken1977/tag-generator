{
  "version": 2,
  "name": "tag-generator-nextjs",
  "builds": [
    {
      "src": "nextjs-app/package.json",
      "use": "@vercel/next",
      "config": {
        "distDir": ".next"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/nextjs-app/$1"
    }
  ],
  "functions": {
    "nextjs-app/app/api/**/*.ts": {
      "maxDuration": 30
    }
  },
  "regions": ["nrt1"]
}